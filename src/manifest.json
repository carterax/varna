{

  "manifest_version": 2,
  "name": "Varna",
  "version": "1.0",

  "description": "Add a list of websites you can easily access by a simple click of a button",
  "homepage_url": "https://github.com/carterax",
  "icons": {
    "48": "images/varna.svg",
    "96": "images/varna.svg"
  },

  "permissions": [
    "activeTab",
    "storage",
    "<all_urls>",
    "cookies",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "images/varna.svg",
    "default_title": "Varna",
    "default_popup": "popup.html"
  }
}